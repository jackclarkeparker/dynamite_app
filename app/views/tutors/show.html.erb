<h1><%= @tutor %></h1>
<div class="show-instance row">
  <div class="col">
    <%= render partial: 'layouts/instance_details', locals: { instance: @tutor } %>

    <div>
      <%= link_to 'Edit this tutor', edit_tutor_path(@tutor) %>
      <%= link_to 'Back to tutors', tutors_path %>

      <%= button_to 'Destroy this tutor', @tutor, method: :delete %>
    </div>
  </div>
</div>
